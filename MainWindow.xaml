<Window x:Class="Homework4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <TextBlock x:Name="zipTextBlock" HorizontalAlignment="Left" Margin="81,58,0,0" 
                   TextWrapping="Wrap" Text="Enter Zip Code" VerticalAlignment="Top" RenderTransformOrigin="0.468,2.199" 
                   Height="19" Width="168"/>

        <TextBox x:Name="uxZipTextBox" MaxLength="10" Height="23"  SelectedText="" AcceptsReturn="True" HorizontalAlignment="Left" Grid.Row="0" 
              VerticalAlignment="Bottom" Width="110" RenderTransformOrigin="0.968,2.046" Margin="81,0,0,314" TextChanged="uxZipTextBox_TextChanged_1" >
            
        </TextBox>
        <Button x:Name ="uxSubmitButton" Content="Submit" IsEnabled="False" HorizontalAlignment="Left" Margin="81,119,0,0" VerticalAlignment="Top" Width="78" 
          RenderTransformOrigin="0.168,-1.343" Height="27" Click="uxSubmitButton_Click"/>




    </Grid>


</Window>
